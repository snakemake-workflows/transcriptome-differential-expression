@article{orjuela_armor_2019,
	title = {{ARMOR}: {An} {A} utomated {R} eproducible {MO} dular {Workflow} for {Preprocessing} and {Differential} {Analysis} of {R} {NA}-seq {Data}},
	volume = {9},
