@article{oleary_exploring_2024-1,
	title = {Exploring and retrieving sequence and metadata for species across the tree of life with {NCBI} {Datasets}},
	volume = {11},
	copyright = {2024 This is a U.S. Government work and not under copyright protection in the US; foreign copyright protection may apply},
