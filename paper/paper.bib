	author = {Orjuela, Stephany and Huang, Ruizhu and Hembach, Katharina M and Robinson, Mark D and Soneson, Charlotte},
	month = jul,
	year = {2019},
	pages = {2089--2096},
	annote = {ARMOR performs an end-to-end RNA-seq data analysis, from raw read files, via quality checks, alignment and quantification, to differential expression testing, geneset analysis and browser-based exploration of the data.
},
	file = {Full Text:/uni-mainz.de/homes/meesters/Zotero/storage/WJH9GSFR/Orjuela et al. - 2019 - ARMOR An A utomated R eproducible MO dular Workfl.pdf:application/pdf},
